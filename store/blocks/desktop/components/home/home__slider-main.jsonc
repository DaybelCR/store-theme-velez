{
    "slider-layout#home__desktop--one": {
        "title":"Home Slider One",
        "props": {
          "itemsPerPage": {
            "desktop": 1,
            "tablet": 1,
            "phone": 1
          },
          "infinite": true,
          "showPaginationDots":"never",
          "showNavigationArrows":"never",
          "blockClass": "home__slider--main",
          "autoplay":{
            "timeout":3000,
            "stopOnHover":true
          }
