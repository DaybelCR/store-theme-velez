        "children": [
          "image#home__slider--one",
          "image#home__slider--two",
          "image#home__slider--three"]
      },
      "image#home__slider--one":{
        "props":{
          "src":"assets/image/desktop/home__1-slider-desktop.png"
        }
      },
      "image#home__slider--two":{
            "props":{
                "src":"assets/image/desktop/home__2-slider-desktop.jpg"
            }
      },
      "image#home__slider--three":{
            "props":{
                "src":"assets/image/desktop/home__3-slider-desktop.jpg"
            }
      }
}